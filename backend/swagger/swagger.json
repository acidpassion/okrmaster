{
    "openapi": "3.0.0",
    "info": {
      "title": "Project Funding Monitoring API",
      "version": "1.0.0",
      "description": "API documentation for the project funding monitoring system"
    },
    "servers": [
      {
        "url": "http://localhost:3000"
      }
    ],
    "paths": {
      "/api/projects": {
        "get": {
          "summary": "Get all projects",
          "responses": {
            "200": {
              "description": "List of all projects"
            }
          }
        }
      },
      "/api/timesheets": {
        "get": {
          "summary": "Get all timesheets",
          "responses": {
